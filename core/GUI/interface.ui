      <property name="styleSheet">
       <string notr="true">/* Style général pour QComboBox */
QComboBox {
    background-color: #ffffff;
    color: #333;
    border: 1px solid #ccc;
    border-radius: 5px;
    padding: 4px 8px;
    font-size: 11px;
}

/* Icône de la flèche */
QComboBox::drop-down {
    border: none;
    background-color: rgb(0, 123, 255);
    border-radius: 0 5px 5px 0; /* Coins arrondis uniquement à droite */
    width: 20px;
}
QComboBox::down-arrow {
    width: 10px;
    height: 10px;
    image: none; /* Supprime l'image si aucune icône n'est fournie */
    border: none;
    color: white;
}

/* Menu déroulant */
QComboBox QAbstractItemView {
    background-color: #ffffff;
    border: 1px solid rgb(0, 123, 255);
    selection-background-color: rgb(0, 123, 255);
    selection-color: #ffffff;
    padding: 5px;
    border-radius: 0px; /* Coins carrés pour le menu */
}
</string>
